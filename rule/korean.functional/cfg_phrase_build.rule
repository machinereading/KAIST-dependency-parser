;;;;;
;;;;; Basic rules
;;;;;

;;; 基本句 ← 自立
( ( [* * * * * ((CONT))] ) BP )

;;; 基本句 ← 基本句 + 付属
( ( [* * * * * ((BP))] [* * * * * ((FUNC))] ) BP )

;;; 基本句 ← 接頭 + 基本句
( ( [* * * * * ((PRE_FUNC))] [* * * * * ((BP))] ) BP )

